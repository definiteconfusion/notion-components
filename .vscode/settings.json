{
    "[rocket-mvbasic]": {
        "editor.codeLens": false
    },
    "files.associations": {
        "*.py": "python",
        ".pyconf": "pyconf",
        "**/.rmv/config/*.json": "jsonc"
    },
    "files.exclude": {
        "**/.git": false,
        "**/.venv": true,
        "**/.idea": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.rmv/catalog/**": true,
        "*/_*": false
    },
    "discord.enabled": true
}